<script>
function SetWinHeight(obj) { 
    var win=obj; 
    var body;
    if (document.getElementById)  { 
        if (win && !window.opera)  { 
            if (win.contentDocument && win.contentDocument.body.offsetHeight) {
                var height = win.contentDocument.body.offsetHeight; 
                win.height = height > 400 ? height : 400;
                body = win.contentDocument.body;
            } else if(win.Document && win.Document.body.scrollHeight) {
                var height = win.Document.body.scrollHeight
                win.height = height > 400 ? height : 400;
                body = win.Document.body;
            }
        } 
    } 
    var scripts = body.getElementsByTagName('script');
    var html = '';
    for(var i = 0; i < scripts.length; i++) {
        if(!scripts[i].src) {
            html = scripts[i].innerHTML;
            break;
        }
    }
    html = html.replace(/(\t|^)\s*\n/g, '');
    var tabNum = /^(\t*)\S/.exec(html)[1];
    if(tabNum) {
        var repStr = '(\\n|^)';
        for(var i = 0; i < tabNum.length; i++) {
            repStr += '\\t';
        }
        var rep = new RegExp(repStr , 'g');
        html = html.replace(rep, function(a,b){return b});

    }
    // html = html.replace(/\n/g, '<br/>');
    // html = html.replace(/\t/g, '&nbsp;&nbsp;&nbsp;&nbsp;');

    $(obj).next().height(win.height).find('code').html(html);

    if(Prism) Prism.highlightAll();
} 

</script>







